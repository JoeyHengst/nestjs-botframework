{
  "name": "nodeconf-template",
  "version": "1.0.0",
  "description": "NodeConf Template",
  "private": true,
  "engines": {
    "node": "^10.0.0",
    "npm": "^6.0.0"
  },
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "npm run tsc",
    "start": "cd ./dist && node -r tsconfig-paths/register ./index.js",
    "dev": "nodemon",
    "prettier": "prettier --write './**/*.*'",
    "tsc": "tsc -p tsconfig.json",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "dev:check": "npm run tslint && npm run tsc"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/platform-fastify": "^6.3.1",
    "botbuilder": "^4.5.3",
    "botbuilder-dialogs": "^4.5.3",
    "botbuilder-storage-mongodb": "^0.9.5",
    "dialogflow": "^0.12.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.0.4",
    "dotenv": "^8.1.0",
    "nodemon": "^1.19.1",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.5.1"
  }
}
